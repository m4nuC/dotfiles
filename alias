#Server Alias
function server() {
        local port="${1:-8000}"
        open "http://localhost:${port}/"
        python -m  SimpleHTTPServer "$port"
}

#folders
function take {
	mkdir $1
	cd $1
}

# List alias
alias la="ls -la"

#exit 
alias x="exit"

#Folders
alias gora="cd /Users/manu/www/_Perso/site_review_app/"
alias gora2="cd /Users/manu/www/_Perso/review_app_l4/"
alias goqc="cd /Users/manu/www/renaud/qcplateform/"
alias selenium="java -jar /usr/local/bin/selenium-server-standalone-2.28.0.jar"
alias restartapache="sudo /usr/sbin/apachectl restart"
alias gitff="git merge --no-ff develop -m 'Rebase on develop'"

#GIT
alias gitlol="git log --graph --decorate --pretty=oneline --abbrev-commit"

#Misc
alias wwwrestart="sudo apachectl start"

